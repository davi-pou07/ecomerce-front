<%- include("./partials/head.ejs")%>


    <%- include("./partials/nav.ejs")%>
        <div class="container">
            <br>
            <div class="row">
                <div class="col-md-4">
                    <ul class="list-group">
                        <li class="list-group-item  bg-primary text-white disabled" aria-disabled="true">A disabled item</li>
                        <li class="list-group-item bg-primary  text-white">A second item</li>
                        <li class="list-group-item  bg-primary  text-white">A third item</li>
                        <li class="list-group-item  bg-primary  text-white">A fourth item</li>
                        <li class="list-group-item  bg-primary  text-white">And a fifth one</li>
                        <li class="list-group-item  bg-primary  text-white">And a fifth one</li>
                        <li class="list-group-item  bg-primary  text-white">And a fifth one</li>
                      </ul>
                </div>
                <div class="col-md-8">
                    <div id="carouselExampleSlidesOnly" class="carousel slide d-block" data-bs-ride="carousel">
                        <div class="carousel-inner">
                            <div class="carousel-item active">
                                <img src="/img/banners/1.jpg" class="d-block w-100" height="300px"
                                    data-bs-interval="5000">
                                <!-- width="1340" height="400" -->
                            </div>
                            <div class="carousel-item">
                                <img src="/img/banners/2.jpg" class="d-block w-100" height="300px"
                                    data-bs-interval="5000">
                            </div>
                            <div class="carousel-item">
                                <img src="/img/banners/3.jpg" class="d-block w-100" height="300px"
                                    data-bs-interval="5000">
                            </div>
                            <div class="carousel-item">
                                <img src="/img/banners/4.jpg" class="d-block w-100" height="300px"
                                    data-bs-interval="5000">
                            </div>
                        </div>
                    </div>
                </div>
                <br>
                <br>
                <div class="row mt-4 pt-5">
                    <%produtos.forEach(produto =>{%>
                    <div class="col-md-4">
                        <div class="card">
                            <div class="card-body">
                                <img src="<%=produto.nome%>" alt="">
                                <input type="text" value="<%=produto.nome%>" disabled>
                            </div>
                        </div>
                    </div>
                    <%})%>
                </div>
            </div>
        </div>

        <%- include("./partials/footer.ejs")%>